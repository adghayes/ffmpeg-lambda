{
  "peaks": {
    "count": 600,
    "quality": 0.8
  },
  "input": {
    "download": {
      "url": "https://www.example.com/download/peaks",
      "headers": {
        "Authorization": "bearer download"
      }
    }
  },
  "outputs": [
    {
      "format": "webm",
      "audio": {
        "bitrate": 96
      },
      "metadata": {
        "platform": "platform"
      },
      "options": ["-dash 1"],
      "upload": {
        "type": "basic",
        "url": "https://www.example.com/upload/peaks",
        "headers": {
          "Authorization": "bearer upload"
        }
      }
    }
  ],
  "callback": {
    "url": "https://www.example.com/info/peaks",
    "headers": {
      "Authorization": "bearer callback"
    },
    "method": "POST"
  }
}
